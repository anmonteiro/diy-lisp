;; function >=
(define >=
	(lambda (a b)
		(or (> a b) (eq a b))))


;; function >=
(define <=
	(lambda (a b)
		(not (> a b))))


;; function >=
(define <
	(lambda (a b)
		(and (not (> a b)) (not (eq a b)))))


;; function sum
(define sum
	(lambda (lst)
		(if (empty lst)
			0
			(+ (head lst) (sum (tail lst))))))


;; function length
(define length
	(lambda (lst)
		(if (empty lst)
			0
			(+ 1 (length (tail lst))))))


;; function append
(define append
	(lambda (lst1 lst2)
		(if (empty lst1)
			lst2
			(cons (head lst1) (append (tail lst1) lst2)))))


;; function filter
(define filter
	(lambda (fn lst)
		(if (empty lst)
			lst
			(if (fn (head lst))
				(cons (head lst) (filter fn (tail lst)))
				(filter fn (tail lst))))))


;; function map
(define map
	(lambda (fn lst)
		(if (empty lst)
			lst
			(cons (fn (head lst)) (map fn (tail lst))))))


;; function reverse
(define reverse
	(lambda (lst)
		(if (empty lst)
			lst
			(append (reverse (tail lst)) (cons (head lst) '())))))


;; function range
(define range
	(lambda (a b)
		(if (> a b)
			'()
			(cons a (range (+ a 1) b)))))


;; function sort
(define sort
	(lambda (lst)
		(if (empty lst)
			lst
			'())))






