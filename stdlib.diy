;; Some logical operators.
(define not
    (lambda (b)
        (if b #f #t)))

;; DIY -- Implement the rest of your standard library
;; here as part 7 of the workshop.

;; function OR
(define or
	(lambda (a b)
		(if a
			#t
			(if b
				#t
				#f))))


;; function AND
(define and
	(lambda (a b)
		(if a
			(if b
				#t
				#f)
			#f)))

